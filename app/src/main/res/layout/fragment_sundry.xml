<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.sundry.SundryFragment">


    <EditText
        android:id="@+id/attribute_text"
        android:layout_width="match_parent"
        android:layout_height="130dp"

        android:enabled="false"
        android:text="仲裁:"
        android:textColor="@color/black"
        android:textSize="12dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="1dp" />

    <EditText
        android:id="@+id/ArchonHuntReward_text"
        android:layout_width="match_parent"
        android:layout_height="181dp"
        android:layout_marginTop="428dp"

        android:enabled="false"
        android:text="执行官:"
        android:textColor="@color/black"
        android:textSize="10dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/sortie_text"
        android:layout_width="match_parent"
        android:layout_height="295dp"
        android:layout_marginTop="132dp"
        android:enabled="false"
        android:maxLines="25"
        android:scrollbars="vertical"
        android:singleLine="false"
        android:text="突击:"
        android:textColor="@color/black"
        android:textSize="12dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/steelpath_text"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:layout_marginTop="4dp"
        android:enabled="false"
        android:textSize="12dp"
        android:text="钢铁之路:"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@+id/ArchonHuntReward_text"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp" />

</androidx.constraintlayout.widget.ConstraintLayout>