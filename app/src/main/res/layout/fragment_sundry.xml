<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.sundry.SundryFragment">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/attribute_text"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"


            android:text="仲裁:"
            android:textColor="@color/black"
            android:textSize="15dp"
            android:background="@drawable/shapedrawable"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/ArchonHuntReward_text"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:background="@drawable/shapedrawable"

            android:text="执行官:"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <TextView
        android:id="@+id/sortie_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

            android:background="@drawable/shapedrawable"

        android:singleLine="false"
        android:text="突击:"
        android:textColor="@color/black"
        android:textSize="15dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
         />

    <TextView
        android:id="@+id/steelpath_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/shapedrawable"

        android:textSize="15dp"
        android:text="钢铁之路:"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@+id/ArchonHuntReward_text"
        tools:ignore="MissingConstraints"
         />
    </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>