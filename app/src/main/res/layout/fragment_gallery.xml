<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.gallery.GalleryFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/Syndicate_cetus_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:text="地球平原赏金:"
                android:textColor="@color/black"

                app:layout_constraintVertical_bias="0.011" />

            <TextView
                android:id="@+id/Syndicate_cetus"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"

                android:gravity="center"

                android:scrollbars="horizontal"
                android:textColor="@color/black"
                app:layout_constraintBottom_toBottomOf="parent"
                android:background="@drawable/shapedrawable"
                tools:ignore="MissingConstraints"></TextView>

            <TextView
                android:id="@+id/Syndicate_vaills_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:text="金星平原赏金:"
                android:textColor="@color/black"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/Syndicate_vaills"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/shapedrawable"

                android:gravity="center"

                android:textColor="@color/black"

                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="0dp" />


            <TextView
                android:id="@+id/Syndicate_cambion_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="火卫二平原赏金:"
                android:textColor="@color/black"

                tools:ignore="MissingConstraints,NotSibling" />

            <TextView
                android:id="@+id/Syndicate_cambion"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Syndicate_cetus"
                android:background="@drawable/shapedrawable"
                android:gravity="center"

                android:textColor="@color/black"

                tools:ignore="MissingConstraints" />


        </LinearLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>