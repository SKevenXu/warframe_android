<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.gallery.GalleryFragment">


    <TextView
        android:id="@+id/Syndicate_cetus_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:textColor="@color/black"
        android:text="地球平原赏金:"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.011" />

    <EditText
        android:id="@+id/Syndicate_cetus"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:enabled="false"
        android:gravity="center"
        android:maxLines="30"

        android:minLines="8"
        android:scrollbars="horizontal"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@id/Syndicate_cetus_title"
        app:layout_constraintTop_toTopOf="@+id/Syndicate_cetus_title"
        app:layout_constraintVertical_bias="0.051"
        tools:ignore="MissingConstraints"></EditText>

    <EditText
        android:id="@+id/Syndicate_vaills"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Syndicate_cetus"
        android:layout_marginTop="48dp"
        android:enabled="false"
        android:gravity="center"
        android:maxLines="30"
        android:minLines="8"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@+id/Syndicate_cetus"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp" />




    <EditText
        android:id="@+id/Syndicate_cambion"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Syndicate_cetus"
        android:layout_marginTop="36dp"
        android:enabled="false"
        android:gravity="center"
        android:maxLines="30"
        android:minLines="8"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@+id/Syndicate_vaills"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-8dp" />


    <TextView
        android:id="@+id/Syndicate_vaills_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/Syndicate_vaills"
        app:layout_constraintTop_toBottomOf="@+id/Syndicate_cetus"
        tools:ignore="MissingConstraints"
        android:text="金星平原赏金:"
        android:textColor="@color/black"
        tools:layout_editor_absoluteX="-8dp" />

    <TextView
        android:id="@+id/Syndicate_cambion_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="火卫二平原赏金:"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/Syndicate_cambion"
        app:layout_constraintTop_toBottomOf="@+id/Syndicate_vaills"
        app:layout_constraintVertical_bias="0.83"
        tools:ignore="MissingConstraints,NotSibling"
        tools:layout_editor_absoluteX="0dp" />
</androidx.constraintlayout.widget.ConstraintLayout>